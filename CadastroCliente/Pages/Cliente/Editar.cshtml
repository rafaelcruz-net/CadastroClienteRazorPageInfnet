@page "{id:int}"
@model CadastroCliente.Pages.Cliente.EditarModel
@{
}

<form method="post">
    <input type="hidden" asp-for="Cliente.Id" />
    <div>
        <label>Nome</label>
        <input type="text" asp-for="Cliente.Nome" />
    </div>
    <div>
        <label>Email</label>
        <input type="text" asp-for="Cliente.Email" />
    </div>
    <div>
        <label>CPF</label>
        <input type="text" asp-for="Cliente.CPF" />
    </div>
    <div>
        <label>Data de Nascimento</label>
        <input type="datetime" asp-for="Cliente.DtNascimento" />
    </div>
    <input type="submit" value="Enviar!" />
</form>

